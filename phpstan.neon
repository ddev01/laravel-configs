includes:
    - vendor/larastan/larastan/extension.neon

parameters:
    paths:
        - app/
    level: 6
    ignoreErrors:
        - '#^Class App\\Enums\\TodoStatus extends generic class BenSampo\\Enum\\Enum but does not specify its types: TValue#'
    excludePaths:
        - bootstrap/app.php
        - _ide_helper_models.php
        - _ide_helper.php
        - .phpstorm.meta.php
        - tests/
